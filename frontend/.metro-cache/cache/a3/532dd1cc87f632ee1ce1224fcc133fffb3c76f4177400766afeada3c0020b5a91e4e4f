) {\n        return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ScrollView.default, {\n          ref: scrollViewRef,\n          style: styles.stepContent,\n          showsVerticalScrollIndicator: false,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.stepTitle,\n            children: \"What are you selling?\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 1244,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.stepSubtitle,\n            children: \"Choose the category and subcategory for your item\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 1245,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.sectionTitle,\n            children: [\"Category \", /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n              style: styles.required,\n              children: \"*\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 1248,\n              columnNumber: 54\n            }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 1248,\n            columnNumber: 11\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: desktopStyles.categoryGrid,\n            children: _srcConfigSubcategories.ALL_CATEGORIES.map(cat => /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n              style: desktopStyles.categoryCardWrapper,\n              children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n                style: [desktopStyles.categoryCardInner, selectedCategoryId === cat.id && desktopStyles.categoryCardSelected],\n                onPress: () => handleCa