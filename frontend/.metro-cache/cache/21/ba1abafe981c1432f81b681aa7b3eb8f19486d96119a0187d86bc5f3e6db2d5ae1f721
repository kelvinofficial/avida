(false);\n          setExpandedSearchMessage(null);\n        }\n        if (refresh) {\n          setListings(listingsRes.listings || listingsRes);\n        } else {\n          setListings(prev => [...prev, ...(listingsRes.listings || listingsRes)]);\n        }\n        setHasMore(listingsRes.page < listingsRes.pages);\n        setCategories(categoriesRes);\n        if (isAuthenticated && !sandboxActive) {\n          try {\n            const favs = await _srcUtilsApi.favoritesApi.getAll();\n            setFavorites(new Set(favs.map(f => f.id)));\n          } catch (e) {}\n          // Also refresh notification count on data refresh\n          fetchNotificationCount();\n        }\n      } catch (error) {\n        console.error('Error fetching data:', error);\n      } finally {\n        setLoading(false);\n        setRefreshing(false);\n        setInitialLoadDone(true);\n      }\n    }, [selectedCategory, currentCity, selectedLocationFilter, selectedCity, includeNearbyCities, searchRadius, page, isAuthenticated, fetchNotificationCount]);\n    (0, _react.useEffect)(() => {\n      fetchData(true);\n    }, [selectedCategory, currentCity, selectedLocationFilter, selectedCity, includeNearbyCities, searchRadius]);\n    const onRefresh = (0, _react.useCallback)(() => {\n      setRefreshing(true);\n      fetchData(true);\n    }, [fetchData]);\n    const loadMore = () => {\n      if (!loading && hasMore) {\n        setPage(prev => prev + 1);\n        fetchData();\n      }\n    };\n    const toggleFavorite = async listingId => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      try {\n        if (favorites.has(listingId)) {\n          await _srcUtilsApi.favoritesApi.remove(listingId);\n          setFavorites(prev => {\n            const newSet = new Set(prev);\n            newSet.delete(listingId);\n            return newSet;\n          });\n        } else {\n          await _srcUtilsApi.favoritesApi.add(listingId);\n          setFa