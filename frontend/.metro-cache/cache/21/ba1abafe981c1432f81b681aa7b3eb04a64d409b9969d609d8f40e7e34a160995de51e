',\n        district_code: location.district_code,\n        district_name: location.district_name,\n        city_code: location.city_code,\n        city_name: location.region_name || location.city_name || '',\n        // Use region_name as fallback\n        lat: location.lat,\n        lng: location.lng,\n        location_text: location.location_text\n      };\n      await saveSelectedCity(locationData);\n    };\n    const handleClearLocationFilter = async () => {\n      setSelectedLocationFilter(null);\n      setCurrentCity('All Locations');\n      setSelectedCity(null);\n      setExpandedSearch(false);\n      setExpandedSearchMessage(null);\n      try {\n        await _srcUtilsStorage.Storage.removeItem('@selected_city');\n      } catch (err) {\n        console.error('Failed to clear saved city:', err);\n      }\n    };\n\n    // ============ HEADER COMPONENT ============\n    const renderHeader = () => /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n      style: styles.headerWrapper,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.row1,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n          style: styles.logo,\n          children: \"avida\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 798,\n          columnNumber: 9\n        }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n          style: styles.notificationButton,\n          onPress: () => router.push('/notifications'),\n          accessibilityLabel: \"Notifications\",\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n            name: \"notifications-outline\",\n            size: ICON_SIZE,\n            color: \"#333\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 804,\n            columnNumber: 11\n          }, this), notificationCount > 0