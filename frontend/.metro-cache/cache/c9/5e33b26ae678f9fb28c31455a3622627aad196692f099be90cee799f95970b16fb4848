\n    // Counter modal state\n    const [showCounterModal, setShowCounterModal] = (0, _react.useState)(false);\n    const [selectedOffer, setSelectedOffer] = (0, _react.useState)(null);\n    const [counterPrice, setCounterPrice] = (0, _react.useState)('');\n    const [counterMessage, setCounterMessage] = (0, _react.useState)('');\n    const [submitting, setSubmitting] = (0, _react.useState)(false);\n\n    // Chat loading state\n    const [openingChatForOffer, setOpeningChatForOffer] = (0, _react.useState)(null);\n    const fetchOffers = (0, _react.useCallback)(async () => {\n      if (!isAuthenticated) {\n        setLoading(false);\n        return;\n      }\n      try {\n        const response = await api.default.get('/offers', {\n          params: {\n            role\n          }\n        });\n        setOffers(response.data.offers || []);\n        setPendingCount(response.data.pending_count || 0);\n      } catch (error) {\n        console.error('Error fetching offers:', error);\n      } finally {\n        setLoading(false);\n        setRefreshing(false);\n      }\n    }, [isAuthenticated, role]);\n    (0, _react.useEffect)(() => {\n      if (isAuthenticated) {\n        setLoading(true);\n        fetchOffers();\n      } else {\n        setLoading(false);\n      }\n    }, [isAuthenticated, role]);\n    const handleRefresh = () => {\n      setRefreshing(true);\n      fetchOffers();\n    };\n    const handleAccept = async offer => {\n      Alert.default.alert('Accept Offer', `Accept ${formatPrice(offer.offered_price)} for \"${offer.listing_title}\"?`, [{\n        text: 'Cancel',\n        style: 'cancel'\n      }, {\n        text: 'Accept',\n        onPress: async () => {\n          try {\n            await api.default.put(`/offers/${offer.id}/respond`, {\n              action: 'accept'\n            });\n            Alert.default.alert('Success', 'Offer accepted!');\n            fetchOffers();\n          } catch (error) {\n            Alert.default.alert('Error', 'Failed to accept offer');\n          }\n        }\n      }]);\n    };\n    const handleReject = async offer => {\n      Alert.default.alert('Decline Offer', `Decline offer of ${formatPrice(offer.offered_price)}?`, [{\n        text: 'Cancel',\n        style: 'cancel'\n      }, {\n        text: 'Decline',\n        style: 'destructive',\n        onPress: async () => {\n          try {\n            await api.default.put(`/offers/${offer.id}/respond`, {\n              action: 'reject'\n            });\n            Alert.default.alert('Done', 'Offer declined');\n            fetchOffers();\n          } catch (error) {\n            Alert.default.alert('Error', 'Failed to decline offer');\n          }\n        }\n      }]);\n    };\n    const handleCounter = offer => {\n      setSelectedOffer(offer);\n      setCounterPrice(String(Math.round((offer.offered_price + offer.listed_price) / 2)));\n      setCounterMessage('');\n      setShowCounterModal(true);\n    };\n    const submitCounter = async () => {\n      if (!selectedOffer || !counterPrice) return;\n      const price = parseInt(counterPrice);\n      if (price <= selectedOffer.offered_price) {\n        Alert.default.alert('Invalid', 'Counter must be higher than the current offer');\n        return;\n      }\n      if (price >= selectedOffer.listed_price) {\n        Alert.default.alert('Invalid', 'Counter must be below the listed price');\n        return;\n      }\n      setSubmitting(true);\n      try {\n        await api.default.put(`/offers/${selectedOffer.id}/respond`, {\n          action: 'counter',\n          counter_price: price,\n          message: counterMessage\n        });\n        setShowCounterModal(false);\n        Alert.default.alert('Success', 'Counter offer sent!');\n        fetchOffers();\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to send counter offer');\n      } finally {\n        setSubmitting(false);\n      }\n    };\n    const handleGoBack = () => {\n      router.canGoBack() ?