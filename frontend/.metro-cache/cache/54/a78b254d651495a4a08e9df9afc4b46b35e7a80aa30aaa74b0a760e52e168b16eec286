s\n          await _srcUtilsApi.conversationsApi.sendMessage(id, `ðŸŽ¤ Voice message (${duration}s)`, 'text');\n        }\n        setMessages(prev => [...prev, {\n          id: `temp_${Date.now()}`,\n          conversation_id: id,\n          sender_id: user?.user_id || '',\n          content: 'Voice message',\n          message_type: 'audio',\n          media_duration: duration,\n          read: false,\n          created_at: new Date().toISOString()\n        }]);\n        setTimeout(() => flatListRef.current?.scrollToEnd({\n          animated: true\n        }), 100);\n      } catch (error) {\n        console.error('Error sending voice message:', error);\n        Alert.default.alert('Error', 'Failed to send voice message');\n      } finally {\n        setSending(false);\n        setRecording(null);\n        setIsRecording(false);\n        setRecordingDuration(0);\n      }\n    };\n\n    // Image/Video Attachment Functions\n    const showAttachmentOptions = () => {\n      if (Platform.default.OS === 'ios') {\n        _reactNativeWebDistIndex.ActionSheetIOS.showActionSheetWithOptions({\n          options: ['Cancel', 'Take Photo', 'Choose from Library', 'Record Video'],\n          cancelButtonIndex: 0\n        }, async buttonIndex => {\n          if (buttonIndex === 1) {\n            await pickImage('camera', 'image');\n          } else if (buttonIndex === 2) {\n            await pickImage('library', 'image');\n          } else if (buttonIndex === 3) {\n            await pickImage('camera', 'video');\n          }\n        });\n      } else {\n        Alert.default.alert('Add Attachment', 'Choose an option', [{\n          text: 'Cancel',\n          style: 'cancel'\n        }, {\n          text: 'Take Photo',\n          onPress: () => pickImage('camera', 'image')\n        }, {\n          text: 'Choose from Library',\n          onPress: () => pickImage('library', 'image')\n        }, {\n          text: 'Record Video',\n          onPress: () => pickImage('camera', 'video')\n        }]);\n      }\n    };\n    const pickImage = async (source, mediaType) => {\n      try {\n        const permissionResult = source === 'camera' ? await ImagePicker.requestCameraPermissionsAsync() : await ImagePicker.requestMediaLibraryPermissionsAsync();\n        if (permissionResult.status !== 'granted') {\n          Alert.default.alert('Permission Required', `Please allow ${source} access to attach ${mediaType}s`);\n          return;\n        }\n        const options = {\n          mediaTypes: mediaType === 'video' ? ['videos'] : ['images'],\n          allowsEditing: true,\n          aspect: [4, 3],\n          quality: 0.8\n        };\n        const result = source === 'camera' ? await ImagePicker.launchCameraAsync(options) : await ImagePicker.launchImageLibraryAsync(options);\n        if (!result.canceled && result.assets[0]) {\n          await sendMediaMessage(result.assets[0], mediaType);\n        }\n      } catch (error) {\n        console.error('Error picking image:', error);\n        Alert.default.alert('Error', 'Failed to pick media');\n      }\n    };\n    const sendMediaMessage = async (asset, mediaType) => {\n      setSending(true);\n      try {\n        const formData = new FormData();\n        const fileName = asset.uri.split('/').pop() || `${mediaType}_${Date.now()}`;\n        const fileType = mediaType === 'image' ? 'image/jpeg' : 'video/mp4';\n        formData.append('file', {\n          uri: asset.uri,\n          type: fileType,\n          name: fileName\n        });\n        try {\n          const uploadResult = await _srcUtilsApi.conversationsApi.uploadMedia(formData, mediaType);\n          await _srcUtilsApi.conversationsApi.sendMessage(id, mediaType === 'image' ? 'Photo' : 'Video', mediaType, uploadResult.media_url);\n        } catch (uploadError) {\n          console.error('Upload failed:', uploadError);\n          // Send base64 as fallback\n          const base64 = asset.base64 || '';\n          await _srcUtilsApi.conversationsApi.sendMessage(id, `ðŸ“· ${mediaType === 'image' ? 'Photo' : 'Video'}`, 'text');\n        }\n        setMessages(prev => [...prev, {\n          id: `temp_${Date.now()}`,\n          conversation_id: id,\n          sender_id: user?.user_id || '',\n          content: mediaType === 'image' ? 'Photo' : 'Video',\n          message_type: mediaType,\n          media_url: asset.uri,\n          read: false,\n          created_at: new Date().toISOString()\n        }]);\n        setTimeout(() => flatListRef.current?.scrollToEnd({\n          animated: true\n        }), 100);\n      } catch (error) {\n        console.error('Error sending media:', error);\n        Alert.default.alert('Error', 'Failed to send media');\n      } finally {\n        setSending(false);\n      }\n    };\n    const formatPrice = price => {\n      return new Intl.NumberFormat('de-DE', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n\n    // Check if current user is the seller (owns the listing)\n    const isSeller = conversation?.listing?.user_id === user?.user_id;\n\n    // Handle offer accept\n    const handleAcceptOffer = async (messageId, amount) => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? {\n          ...msg,\n          offer_status: 'accepted'\n        } : msg));\n\n        // Send acceptance message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âœ… OFFER ACCEPTED!\\n\\nI've accepted your offer of ${amount}. Let's arrange the details!`);\n        Alert.default.alert('Offer Accepted', `You've accepted the offer of ${amount}`);\n      } catch (error) {\n        console.error('Error accepting offer:', error);\n        Alert.default.alert('Error', 'Failed to accept offer');\n      }\n    };\n\n    // Handle offer reject\n    const handleRejectOffer = async messageId => {\n      try {\n        // Update message status locally\n        s