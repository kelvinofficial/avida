      conversation_id: id,\n          sender_id: user?.user_id || '',\n          content: mediaType === 'image' ? 'Photo' : 'Video',\n          message_type: mediaType,\n          media_url: asset.uri,\n          read: false,\n          created_at: new Date().toISOString()\n        }]);\n        setTimeout(() => flatListRef.current?.scrollToEnd({\n          animated: true\n        }), 100);\n      } catch (error) {\n        console.error('Error sending media:', error);\n        Alert.default.alert('Error', 'Failed to send media');\n      } finally {\n        setSending(false);\n      }\n    };\n    const formatPrice = price => {\n      return new Intl.NumberFormat('de-DE', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n\n    // Check if current user is the seller (owns the listing)\n    const isSeller = conversation?.listing?.user_id === user?.user_id;\n\n    // Handle offer accept\n    const handleAcceptOffer = async (messageId, amount) => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? {\n          ...msg,\n          offer_status: 'accepted'\n        } : msg));\n\n        // Send acceptance message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âœ… OFFER ACCEPTED!\\n\\nI've accepted your offer of ${amount}. Let's arrange the details!`);\n        Alert.default.alert('Offer Accepted', `You've accepted the offer of ${amount}`);\n      } catch (error) {\n        console.error('Error accepting offer:', error);\n        Alert.default.alert('Error', 'Failed to accept offer');\n      }\n    };\n\n    // Handle offer reject\n    const handleRejectOffer = async messageId => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? {\n          ...msg,\n          offer_status: 'rejected'\n        } : msg));\n\n        // Send rejection message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âŒ OFFER DECLINED\\n\\nThank you for your interest, but I cannot accept this offer. Feel free to make a higher offer!`);\n      } catch (error) {\n        console.error('Error rejecting offer:', error);\n        Alert.default.alert('Error', 'Failed to reject offer');\n      }\n    };\n\n    // Add date separators between messages\n    const getMessagesWithSeparators = (0, _react.useCallback)(() => {\n      const result = [];\n      let lastDate = null;\n      messages.forEach(msg => {\n        // Ensure the string is treated as UTC if no timezone specified\n        const d = new Date(msg.created_at.endsWith('Z') ? msg.created_at : msg.created_at + 'Z');\n        const msgDate = (0, _dateFns.format)(d, 'yyyy-MM-dd');\n        if (msgDate !== lastDate) {\n          result.push({\n            type: 'date',\n            date: msg.created_at\n          });\n          lastDate = msgDate;\n        }\n        result.push(msg);\n      });\n      return result;\n    }, [messages]);\n    const renderItem = (0, _react.useCallback)(({\n      item,\n      index\n    }) => {\n      if (item.type === 'date') {\n        return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(DateSeparator, {\n          date: item.date\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 1749,\n          columnNumber: 16\n        }, this);\n      }\n      const message = item;\n      const isMine = message.sender_id === user?.user_id;\n      const prevItem = getMessagesWithSeparators()[index - 1];\n      const showAvatar = !isMine && (index === 0 || prevItem?.type === 'date' || prevItem?.sender_id !== message.sender_id);\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(MessageBubble, {\n        message: message,\n        isMine: isMine,\n        showAvatar: showAvatar,\n        otherUser: conversation?.other_user,\n        isSeller: isSeller,\n        onAcceptOffer: handleAcceptOffer,\n        onRej