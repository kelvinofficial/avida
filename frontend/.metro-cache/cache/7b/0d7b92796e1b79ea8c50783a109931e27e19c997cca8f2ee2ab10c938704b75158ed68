}\n        const options = {\n          mediaTypes: mediaType === 'video' ? ['videos'] : ['images'],\n          allowsEditing: true,\n          aspect: [4, 3],\n          quality: 0.8\n        };\n        const result = source === 'camera' ? await ImagePicker.launchCameraAsync(options) : await ImagePicker.launchImageLibraryAsync(options);\n        if (!result.canceled && result.assets[0]) {\n          await sendMediaMessage(result.assets[0], mediaType);\n        }\n      } catch (error) {\n        console.error('Error picking image:', error);\n        Alert.default.alert('Error', 'Failed to pick media');\n      }\n    };\n    const sendMediaMessage = async (asset, mediaType) => {\n      setSending(true);\n      try {\n        const formData = new FormData();\n        const fileName = asset.uri.split('/').pop() || `${mediaType}_${Date.now()}`;\n        const fileType = mediaType === 'image' ? 'image/jpeg' : 'video/mp4';\n        formData.append('file', {\n          uri: asset.uri,\n          type: fileType,\n          name: fileName\n        });\n        try {\n          const uploadResult = await _srcUtilsApi.conversationsApi.uploadMedia(formData, mediaType);\n          await _srcUtilsApi.conversationsApi.sendMessage(id, mediaType === 'image' ? 'Photo' : 'Video', mediaType, uploadResult.media_url);\n        } catch (uploadError) {\n          console.error('Upload failed:', uploadError);\n          // Send base64 as fallback\n          const base64 = asset.base64 || '';\n          await _srcUtilsApi.conversationsApi.sendMessage(id, `ðŸ“· ${mediaType === 'image' ? 'Photo' : 'Video'}`, 'text');\n        }\n        setMessages(prev => [...prev, {\n          id: `temp_${Date.now()}`,\n          conversation_id: id,\n          sender_id: user?.user_id || '',\n          content: mediaType === 'image' ? 'Photo' : 'Video',\n          message_type: mediaType,\n          media_url: asset.uri,\n          read: false,\n          created_at: new Date().toISOString()\n        }]);\n        setTimeout(() => flatListRef.current?.scrollToEnd({\n          animated: true\n        }), 100);\n      } catch (error) {\n        console.error('Error sending media:', error);\n        Alert.default.alert('Error', 'Failed to send media');\n      } finally {\n        setSending(false);\n      }\n    };\n    const formatPrice = price => {\n      return new Intl.NumberFormat('de-DE', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n\n    // Check if current user is the seller (owns the listing)\n    const isSeller = conversation?.listing?.user_id === user?.user_id;\n\n    // Handle offer accept\n    const handleAcceptOffer = async (messageId, amount) => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? {\n          ...msg,\n          offer_status: 'accepted'\n        } : msg));\n\n        // Send acceptance message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âœ… OFFER ACCEPTED!\\n\\nI've accepted your offer of ${amount}. Let's arrange the details!`);\n        Alert.default.alert('Offer Accepted', `You've accepted the offer of ${amount}`);\n      } catch (error) {\n        console.error('Error accepting offer:', error);\n        Alert.default.alert('Error', 'Failed to accept offer');\n      }\n    };\n\n    // Handle offer reject\n    const handleRejectOffer = async messageId => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? {\n          ...msg,\n          offer_status: 'rejected'\n        } : msg));\n\n        // Send rejection message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âŒ OFFER DECLINED\\n\\nThank you for your interest, but I cannot accept this offer. Feel free to make a higher offer!`);\n      } catch (error) {\n        console.error('Error rejecting offer:', error);\n        Alert.defaul