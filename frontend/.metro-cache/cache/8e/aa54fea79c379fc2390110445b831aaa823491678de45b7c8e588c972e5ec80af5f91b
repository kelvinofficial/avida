ive layout is ready (prevents layout flash)\n    if (!isReady) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: [styles.container, {\n          backgroundColor: '#F0F2F5'\n        }],\n        edges: ['top'],\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: {\n            flex: 1,\n            justifyContent: 'center',\n            alignItems: 'center'\n          },\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n            size: \"large\",\n            color: COLORS.primary\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 663,\n            columnNumber: 11\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 662,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 661,\n        columnNumber: 7\n      }, this);\n    }\n    if (!isAuthenticated) {\n      // Use the unified AuthPrompt for both mobile and desktop\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_srcComponentsAuthPrompt.AuthPrompt, {\n        title: \"Your Messages\",\n        subtitle: \"Sign in to chat with buyers and sellers, negotiate prices, and close deals\",\n        icon: \"chatbubbles-outline\",\n        redirectPath: \"/messages\"\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 672,\n        columnNumber: 7\n      }, this);\n    }\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_srcComponentsSkeletons.MessagesSkeleton, {\n        isDesktop: isLargeScreen\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 682,\n        columnNumber: 12\n      }, this);\n    }\n    const emptyState = getEmptyState();\n\n    // Desktop: Master-Detail Layout with Embedded Chat