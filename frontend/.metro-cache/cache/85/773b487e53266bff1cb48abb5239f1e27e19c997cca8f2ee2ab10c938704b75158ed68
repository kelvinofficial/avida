;\n        Alert.default.alert('Error', 'Failed to accept offer');\n      }\n    };\n\n    // Handle offer reject\n    const handleRejectOffer = async messageId => {\n      try {\n        // Update message status locally\n        setMessages(prev => prev.map(msg => msg.id === messageId ? Object.assign({}, msg, {\n          offer_status: 'rejected'\n        }) : msg));\n\n        // Send rejection message\n        await _srcUtilsApi.conversationsApi.sendMessage(id, `âŒ OFFER DECLINED\\n\\nThank you for your interest, but I cannot accept this offer. Feel free to make a higher offer!`);\n      } catch (error) {\n        console.error('Error rejecting offer:', error);\n        Alert.default.alert('Error', 'Failed to reject offer');\n      }\n    };\n\n    // Add date separators between messages\n    const getMessagesWithSeparators = (0, _react.useCallback)(() => {\n      const result = [];\n      let lastDate = null;\n      messages.forEach(msg => {\n        // Ensure the string is treated as UTC if no timezone specified\n        const d = new Date(msg.created_at.endsWith('Z') ? msg.created_at : msg.created_at + 'Z');\n        const msgDate = (0, _dateFns.format)(d, 'yyyy-MM-dd');\n        if (msgDate !== lastDate) {\n          result.push({\n            type: 'date',\n            date: msg.created_at\n          });\n          lastDate = msgDate;\n        }\n        result.push(msg);\n      });\n      return result;\n    }, [messages]);\n    const renderItem = (0, _react.useCallback)(({\n      item,\n      index\n    }) => {\n      if (item.type === 'date') {\n        return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(DateSeparator, {\n          date: item.date\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 1906,\n          columnNumber: 16\n        }, this);\n      }\n      const message = item;\n      const isMine = message.sender_id === user?.user_id;\n      const prevItem = getMessagesWithSeparators()[index - 1];\n      const