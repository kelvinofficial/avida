;\n    const {\n      isDesktop,\n      isTablet,\n      isReady\n    } = (0, _srcHooksUseResponsive.useResponsive)();\n    const isLargeScreen = isDesktop || isTablet;\n    const [conversations, setConversations] = (0, _react.useState)([]);\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [refreshing, setRefreshing] = (0, _react.useState)(false);\n    const [activeFilter, setActiveFilter] = (0, _react.useState)('all');\n    const [searchQuery, setSearchQuery] = (0, _react.useState)('');\n    const [selectedConversation, setSelectedConversation] = (0, _react.useState)(null);\n    const [isSandbox, setIsSandbox] = (0, _react.useState)(false);\n\n    // Embedded chat states\n    const [chatMessages, setChatMessages] = (0, _react.useState)([]);\n    const [chatLoading, setChatLoading] = (0, _react.useState)(false);\n    const [newMessage, setNewMessage] = (0, _react.useState)('');\n    const [sending, setSending] = (0, _react.useState)(false);\n    const chatListRef = (0, _react.useRef)(null);\n    const socketRef = (0, _react.useRef)(null);\n\n    // User online status tracking\n    const [userStatuses, setUserStatuses] = (0, _react.useState)({});\n    const fetchConversations = (0, _react.useCallback)(async () => {\n      // Check sandbox mode\n      const sandboxActive = await _srcUtilsSandboxAwareApi.sandboxUtils.isActive();\n      setIsSandbox(sandboxActive);\n      if (!sandboxActive && !isAuthenticated) {\n        setLoading(false);\n        return;\n      }\n      try {\n        let data = [];\n        if (sandboxActive) {\n          // Fetch sandbox conversations\n          const session = await _srcUtilsSandboxAwareApi.sandboxUtils.getSession();\n          const userId = session?.sandbox_user_id;\n          if (userId) {\n            const response = await api.default.get(`/sandbox/proxy/conversations/${userId}`);\n            const rawConversations = response.data || [];\n\n            // Transform sandbox conversations to match expected format\n            data = rawConversations.map((conv, index) => ({\n              ...conv,\n              other_user: {\n                user_id: conv.participants?.find(p => p !== userId) || `sandbox_user_${index}`,\n                name: `[SANDBOX] User ${index + 1}`,\n                avatar: `https://i.pravatar.cc/150?u=${conv.id}`\n              },\n              last_message: conv.last_message || {\n                text: '[SANDBOX] Sample conversation',\n                created_at: conv.created_at || new Date().toISOString()\n              },\n              unread: 0,\n              sandbox_mode: true\n            }));\n          }\n        } else {\n          // Normal production flow\n          data = await _srcUtilsApi.conversationsApi.getAll();\n\n          // Fetch online status for all conversation users\n          const userIds = data.map(c => c.other_user?.user_id).filter(id => !!id);\n          if (userIds.length > 0) {\n            try {\n              const statuses = await _srcUtilsApi.usersApi.getStatusBatch(userIds);\n              setUserStatuses(statuses);\n            } catch (err) {\n              console.error('Error fetching user statuses:', err);\n            }\n          }\n        }\n        setConversations(data);\n      } catch (error) {\n        console.error('Error fetching conversations:', error);\n      } finally {\n        setLoading(false);\n        setRefreshing(false);\n      }\n    }, [isAuthenticated]);\n    (0, _reactNavigationNative.useFocusEffect)((0, _react.useCallback)(() => {\n      fetchConversations();\n    }, [fetchConversations]));\n    const onRefresh = () => {\n      setRefreshing(true);\n      fetchConversations();\n    };\n\n    // Calculate unread count\n    const unreadCount = conversations.reduce((acc, c) => acc + (c.unread || 0), 0);\n\n    // Filter conversations\n    const filteredConversations = conversations.filter(c => {\n      // Search filter\n      if (searchQuery) {\n        const query = searc