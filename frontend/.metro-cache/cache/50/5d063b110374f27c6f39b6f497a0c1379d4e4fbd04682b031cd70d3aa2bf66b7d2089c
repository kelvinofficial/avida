 ============ MAIN SCREEN ============\n  function ListingDetailScreen() {\n    const {\n      id\n    } = (0, _expoRouter.useLocalSearchParams)();\n    const router = (0, _expoRouter.useRouter)();\n    const {\n      isAuthenticated,\n      user\n    } = (0, _srcStoreAuthStore.useAuthStore)();\n    const [listing, setListing] = (0, _react.useState)(null);\n    const [category, setCategory] = (0, _react.useState)(null);\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [isFavorited, setIsFavorited] = (0, _react.useState)(false);\n    const [showReportModal, setShowReportModal] = (0, _react.useState)(false);\n    const [showOfferModal, setShowOfferModal] = (0, _react.useState)(false);\n    const [showOfferSuccessModal, setShowOfferSuccessModal] = (0, _react.useState)(false);\n    const [offerPrice, setOfferPrice] = (0, _react.useState)('');\n    const [offerMessage, setOfferMessage] = (0, _react.useState)('');\n    const [submittedOfferAmount, setSubmittedOfferAmount] = (0, _react.useState)('');\n    const [offerConversationId, setOfferConversationId] = (0, _react.useState)(null);\n    const [submittingOffer, setSubmittingOffer] = (0, _react.useState)(false);\n    const fetchListing = (0, _react.useCallback)(async () => {\n      try {\n        setLoading(true);\n        const data = await _srcUtilsApi.listingsApi.getOne(id);\n        setListing(data);\n        setIsFavorited(data.is_favorited || false);\n\n        // Track recently viewed (don't await, fire and forget)\n        if (isAuthenticated) {\n          api.default.post(`/profile/activity/recently-viewed/${id}`).catch(() => {});\n        }\n        try {\n          const catData = await _srcUtilsApi.categoriesApi.getOne(data.category_id);\n          setCategory(catData);\n        } catch (e) {}\n      } catch (error) {\n        console.error('Error fetching listing:', error);\n        Alert.default.alert('Error', 'Failed to load listing');\n      } finally {\n        setLoading(false);\n      }\n    }, [id, isAuthenticated]);\n    (0, _react.useEffect)(() => {\n      if (id) fetchListing();\n    }, [id, fetchListing]);\n    const formatPrice = price => {\n      return new Intl.NumberFormat('de-DE', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n    const getTimeAgo = date => {\n      try {\n        return (0, _dateFns.formatDistanceToNow)(new Date(date), {\n          addSuffix: true\n        });\n      } catch {\n        return '';\n      }\n    };\n    const handleToggleFavorite = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      const wasFavorited = isFavorited;\n      setIsFavorited(!isFavorited);\n      try {\n        if (wasFavorited) await _srcUtilsApi.favoritesApi.remove(id);else await _srcUtilsApi.favoritesApi.add(id);\n      } catch (error) {\n        setIsFavorited(wasFavorited);\n      }\n    };\n    const handleShare = async () => {\n      if (!listing) return;\n      try {\n        await Share.default.share({\n          message: `Check out: ${listing.title} - ${formatPrice(listing.price)}`\n        });\n      } catch (error) {}\n    };\n    const handleChat = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      if (listing?.user_id === user?.user_id) {\n        Alert.default.alert('Info', 'This is your own listing');\n        return;\n      }\n      try {\n        const conversation = await _srcUtilsApi.conversationsApi.create(id);\n        router.push(`/chat/${conversation.id}`);\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to start chat');\n      }\n    };\n    const handleReport = async reason => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      try {\n        await _srcUtilsApi.reportsApi.create({\n          listing_id: id,\n          reason,\n          description: 