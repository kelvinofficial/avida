', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n    const getTimeAgo = date => {\n      try {\n        return (0, _dateFns.formatDistanceToNow)(new Date(date), {\n          addSuffix: true\n        });\n      } catch {\n        return '';\n      }\n    };\n    const handleToggleFavorite = async () => {\n      if (!isAuthenticated) {\n        router.push(`/login?redirect=${encodeURIComponent(`/listing/${id}`)}`);\n        return;\n      }\n      const wasFavorited = isFavorited;\n      setIsFavorited(!isFavorited);\n      try {\n        if (wasFavorited) await _srcUtilsApi.favoritesApi.remove(id);else await _srcUtilsApi.favoritesApi.add(id);\n      } catch (error) {\n        setIsFavorited(wasFavorited);\n      }\n    };\n    const handleShare = async () => {\n      if (!listing) return;\n      try {\n        await Share.default.share({\n          message: `Check out: ${listing.title} - ${formatPrice(listing.price)}`\n        });\n      } catch (error) {}\n    };\n    const handleChat = async () => {\n      if (!isAuthenticated) {\n        router.push(`/login?redirect=${encodeURIComponent(`/listing/${id}`)}`);\n        return;\n      }\n      if (listing?.user_id === user?.user_id) {\n        Alert.default.alert('Info', 'This is your own listing');\n        return;\n      }\n      try {\n        const conversation = await _srcUtilsApi.conversationsApi.create(id);\n        router.push(`/chat/${conversation.id}`);\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to start chat');\n      }\n    };\n    const handleReport = async reason => {\n      if (!isAuthenticated) {\n        router.push(`/login?redirect=${encodeURIComponent(`/listing/${id}`)}`);\n        return;\n      }\n      try {\n        await _srcUtilsApi.reportsApi.create({\n          listing_id: id,\n          reason,\n          description: `Reported listing: ${listing?.title}`\n        });\n        setShowR