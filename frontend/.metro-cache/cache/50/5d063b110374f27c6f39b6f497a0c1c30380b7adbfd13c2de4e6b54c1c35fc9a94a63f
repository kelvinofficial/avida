    const [offerMessage, setOfferMessage] = (0, _react.useState)('');\n    const [submittedOfferAmount, setSubmittedOfferAmount] = (0, _react.useState)('');\n    const [offerConversationId, setOfferConversationId] = (0, _react.useState)(null);\n    const [submittingOffer, setSubmittingOffer] = (0, _react.useState)(false);\n    const [currentImageIndex, setCurrentImageIndex] = (0, _react.useState)(0);\n    const [canBuyOnline, setCanBuyOnline] = (0, _react.useState)(false);\n    const fetchListing = (0, _react.useCallback)(async () => {\n      try {\n        setLoading(true);\n\n        // Check if sandbox mode is active and use sandbox-aware API\n        const sandboxActive = await _srcUtilsSandboxAwareApi.sandboxUtils.isActive();\n        let data;\n        if (sandboxActive) {\n          data = await _srcUtilsSandboxAwareApi.sandboxAwareListingsApi.getOne(id);\n        } else {\n          data = await _srcUtilsApi.listingsApi.getOne(id);\n        }\n        setListing(data);\n        setIsFavorited(data.is_favorited || false);\n\n        // Track recently viewed (don't await, fire and forget) - skip in sandbox\n        if (isAuthenticated && !sandboxActive) {\n          api.default.post(`/profile/activity/recently-viewed/${id}`).catch(() => {});\n        }\n\n        // Skip category fetch in sandbox (not critical)\n        if (!sandboxActive) {\n          try {\n            const catData = await _srcUtilsApi.categoriesApi.getOne(data.category_id);\n            setCategory(catData);\n          } catch (e) {}\n\n          // Check if seller can sell online\n          try {\n            const sellerCheck = await api.default.get(`/escrow/seller/${data.user_id}/can-sell-online`);\n            setCanBuyOnline(sellerCheck.data.can_sell_online === true);\n          } catch (e) {\n            setCanBuyOnline(false);\n          }\n        } else {\n          // In sandbox mode, enable buy online for testing\n          setCanBuyOnline(true);\n        }\n      } catch (error