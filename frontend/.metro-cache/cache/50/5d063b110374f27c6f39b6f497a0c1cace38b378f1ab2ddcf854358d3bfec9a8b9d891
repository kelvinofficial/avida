 ============ MAIN SCREEN ============\n  function ListingDetailScreen() {\n    const {\n      id\n    } = (0, _expoRouter.useLocalSearchParams)();\n    const router = (0, _expoRouter.useRouter)();\n    const {\n      isAuthenticated,\n      user\n    } = (0, _srcStoreAuthStore.useAuthStore)();\n    const [listing, setListing] = (0, _react.useState)(null);\n    const [category, setCategory] = (0, _react.useState)(null);\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [isFavorited, setIsFavorited] = (0, _react.useState)(false);\n    const [showReportModal, setShowReportModal] = (0, _react.useState)(false);\n    const [showOfferModal, setShowOfferModal] = (0, _react.useState)(false);\n    const [showOfferSuccessModal, setShowOfferSuccessModal] = (0, _react.useState)(false);\n    const [offerPrice, setOfferPrice] = (0, _react.useState)('');\n    const [offerMessage, setOfferMessage] = (0, _react.useState)('');\n    const [submittedOfferAmount, setSubmittedOfferAmount] = (0, _react.useState)('');\n    const [offerConversationId, setOfferConversationId] = (0, _react.useState)(null);\n    const [submittingOffer, setSubmittingOffer] = (0, _react.useState)(false);\n    const fetchListing = (0, _react.useCallback)(async () => {\n      try {\n        setLoading(true);\n        const data = await _srcUtilsApi.listingsApi.getOne(id);\n        setListing(data);\n        setIsFavorited(data.is_favorited || false);\n\n        // Track recently viewed (don't await, fire and forget)\n        if (isAuthenticated) {\n          api.default.post(`/profile/activity/recently-viewed/${id}`).catch(() => {});\n        }\n        try {\n          const catData = await _srcUtilsApi.categoriesApi.getOne(data.category_id);\n          setCategory(catData);\n        } catch (e) {}\n      } catch (error) {\n        console.error('Error fetching listing:', error);\n        Alert.default.alert('Error', 'Failed to load listing');\n      } finally {\n        setLoading(false);\n      }\n    }, [id, isAuthenticated]);\n    (0, _react.useEffect)(() => {\n      if (id) fetchListing();\n    }, [id, fetchListing]);\n    const formatPrice = price => {\n      return new Intl.NumberFormat('de-DE', {\n        style: 'currency',\n        currency: 'EUR',\n        minimumFractionDigits: 0\n      }).format(price);\n    };\n    const getTimeAgo = date => {\n      try {\n        return (0, _dateFns.formatDistanceToNow)(new Date(date), {\n          addSuffix: true\n        });\n      } catch {\n        return '';\n      }\n    };\n    const handleToggleFavorite = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      const wasFavorited = isFavorited;\n      setIsFavorited(!isFavorited);\n      try {\n        if (wasFavorited) await _srcUtilsApi.favoritesApi.remove(id);else await _srcUtilsApi.favoritesApi.add(id);\n      } catch (error) {\n        setIsFavorited(wasFavorited);\n      }\n    };\n    const handleShare = async () => {\n      if (!listing) return;\n      try {\n        await Share.default.share({\n          message: `Check out: ${listing.title} - ${formatPrice(listing.price)}`\n        });\n      } catch (error) {}\n    };\n    const handleChat = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      if (listing?.user_id === user?.user_id) {\n        Alert.default.alert('Info', 'This is your own listing');\n        return;\n      }\n      try {\n        const conversation = await _srcUtilsApi.conversationsApi.create(id);\n        router.push(`/chat/${conversation.id}`);\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to start chat');\n      }\n    };\n    const handleReport = async reason => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      try {\n        await _srcUtilsApi.reportsApi.create({\n          listing_id: id,\n          reason,\n          description: `Reported listing: ${listing?.title}`\n        });\n        setShowReportModal(false);\n        Alert.default.alert('Report Submitted', 'Thank you for helping keep our community safe.');\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to submit report');\n      }\n    };\n    const handleCall = () => {\n      if (!listing?.seller?.phone) {\n        Alert.default.alert('Error', 'Phone number not available');\n        return;\n      }\n      Linking.default.openURL(`tel:${listing.seller.phone}`);\n    };\n    const handleWhatsApp = () => {\n      if (!listing) return;\n      const phone = listing.seller?.whatsapp || listing.seller?.phone || '';\n      if (!phone) {\n        Alert.default.alert('Error', 'WhatsApp number not available');\n        return;\n      }\n      const cleanPhone = phone.replace(/[^0-9]/g, '');\n      const message = encodeURIComponent(`Hi, I'm interested in your \"${listing.title}\" listed for ${formatPrice(listing.price)}. Is it still available?`);\n      Linking.default.openURL(`https://wa.me/${cleanPhone}?text=${message}`);\n    };\n    const handleSubmitOffer = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      if (!offerPrice) {\n        Alert.default.alert('Error', 'Please enter an offer amount');\n        return;\n      }\n      const offerAmount = parseInt(offerPrice);\n\n      // Validate offer is below listed price\n      if (offerAmount >= listing.price) {\n        Alert.default.alert('Invalid Offer', `Your offer must be below the listed price of ${formatPrice(listing.price)}. If you want to pay full price, use the \"Contact Seller\" option instead.`);\n        return;\n      }\n\n      // Validate offer is reasonable (at least 10% of listed price)\n      const minOffer = listing.price * 0.1;\n      if (offerAmount < minOffer) {\n        Alert.default.alert('Offer Too Low', `Please enter a reasonable offer. Minimum offer is ${formatPrice(minOffer)}`);\n        return;\n      }\n      setSubmittingOffer(true);\n      try {\n        // Save offer to database\n        await api.default.post('/offers', {\n          listing_id: id,\n          offered_price: offerAmount,\n          message: offerMessage || 'I would like to make an offer on this item.'\n        });\n\n        // Start a conversation with the offer message\n        const conversation = await _srcUtilsApi.conversationsApi.create(id);\n\n        // Send the offer message automatically\n        const formattedOffer = formatPrice(offerAmount);\n        const discount = Math.round((1 - offerAmount / listing.price) * 100);\n        const offerText = ` OFFER SUBMITTED\\n\\nAmount: ${formattedOffer} (${discount}% off)\\nListed Price: ${formatPrice(listing.price)}\\n\\n${offerMessage || 'I would like to make an offer on this item.'}`;\n        await _srcUtilsApi.conversationsApi.sendMessage(conversation.id, offerText);\n\n        // Store offer details for success modal\n        setSubmittedOfferAmount(formattedOffer);\n        setOfferConversationId(conversation.id);\n\n        // Close offer modal and show success modal\n        setShowOfferModal(false);\n        setOfferPrice('');\n        setOfferMessage('');\n        setShowOfferSuccessModal(true);\n      } catch (error) {\n        console.error('Error sending offer:', error);\n        Alert.default.alert('Error', 'Failed to send offer. Please try again.');\n      } finally {\n        setSubmittingOffer(false);\n      }\n    };\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.loading,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n            size: \"large\",\n            color: COLORS.primary\n          }, void 0, false, {\n            fileName: _jsxF