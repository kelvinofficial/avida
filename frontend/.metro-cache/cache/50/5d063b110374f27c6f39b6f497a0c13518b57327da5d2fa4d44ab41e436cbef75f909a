}\n    };\n    const handleChat = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      if (listing?.user_id === user?.user_id) {\n        Alert.default.alert('Info', 'This is your own listing');\n        return;\n      }\n      try {\n        const conversation = await _srcUtilsApi.conversationsApi.create(id);\n        router.push(`/chat/${conversation.id}`);\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to start chat');\n      }\n    };\n    const handleReport = async reason => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        return;\n      }\n      try {\n        await _srcUtilsApi.reportsApi.create({\n          listing_id: id,\n          reason,\n          description: `Reported listing: ${listing?.title}`\n        });\n        setShowReportModal(false);\n        Alert.default.alert('Report Submitted', 'Thank you for helping keep our community safe.');\n      } catch (error) {\n        Alert.default.alert('Error', 'Failed to submit report');\n      }\n    };\n    const handleCall = () => {\n      if (!listing?.seller?.phone) {\n        Alert.default.alert('Error', 'Phone number not available');\n        return;\n      }\n      Linking.default.openURL(`tel:${listing.seller.phone}`);\n    };\n    const handleWhatsApp = () => {\n      if (!listing) return;\n      const phone = listing.seller?.whatsapp || listing.seller?.phone || '';\n      if (!phone) {\n        Alert.default.alert('Error', 'WhatsApp number not available');\n        return;\n      }\n      const cleanPhone = phone.replace(/[^0-9]/g, '');\n      const message = encodeURIComponent(`Hi, I'm interested in your \"${listing.title}\" listed for ${formatPrice(listing.price)}. Is it still available?`);\n      Linking.default.openURL(`https://wa.me/${cleanPhone}?text=${message}`);\n    };\n    const handleSubmitOffer = async () => {\n      if (!isAuthenticated) {\n        router.push('/login');\n        