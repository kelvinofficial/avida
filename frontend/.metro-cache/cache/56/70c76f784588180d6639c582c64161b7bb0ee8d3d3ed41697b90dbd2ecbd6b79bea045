    }\n  });\n\n  // Main Chat Screen\n  function ChatScreen() {\n    _s4();\n    const {\n      id\n    } = (0, _expoRouter.useLocalSearchParams)();\n    const router = (0, _expoRouter.useRouter)();\n    const {\n      user\n    } = (0, _srcStoreAuthStore.useAuthStore)();\n    const {\n      isDesktop,\n      isTablet,\n      isReady\n    } = (0, _srcHooksUseResponsive.useResponsive)();\n    const isLargeScreen = isDesktop || isTablet;\n    const [conversation, setConversation] = (0, _react.useState)(null);\n    const [messages, setMessages] = (0, _react.useState)([]);\n    const [newMessage, setNewMessage] = (0, _react.useState)('');\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [sending, setSending] = (0, _react.useState)(false);\n    const [isTyping, setIsTyping] = (0, _react.useState)(false);\n    const [typingUser, setTypingUser] = (0, _react.useState)(undefined);\n\n    // Voice recording states\n    const [isRecording, setIsRecording] = (0, _react.useState)(false);\n    const [recordingDuration, setRecordingDuration] = (0, _react.useState)(0);\n    const [recording, setRecording] = (0, _react.useState)(null);\n    const flatListRef = (0, _react.useRef)(null);\n    const socketRef = (0, _react.useRef)(null);\n    const typingTimeoutRef = (0, _react.useRef)(null);\n    const recordingTimerRef = (0, _react.useRef)(null);\n    (0, _react.useEffect)(() => {\n      if (id) {\n        fetchConversation();\n        connectSocket();\n      }\n      return () => {\n        if (socketRef.current) {\n          socketRef.current.emit('leave_conversation', {\n            conversation_id: id\n          });\n          socketRef.current.disconnect();\n        }\n        if (typingTimeoutRef.current) {\n          clearTimeout(typingTimeoutRef.current);\n        }\n        if (recordingTimerRef.current) {\n          clearInterval(recordingTimerRef.current);\n        }\n        // Stop any active recording\n        if (recording) {\n          recordi